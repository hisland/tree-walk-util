{
  "compilerOptions": {
    "baseUrl": ".",
    // "outDir": "dist",
    "sourceMap": false,
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": false,
    "jsx": "preserve",
    "rootDir": "."
  },
  "include": ["src"]
}
